function change(val, obj){
	if(val=='homeTag'){
		homeTag.style.color='white';
	}else if(val=='projects'){
		projects.style.color='red';
		homeTag.style.color='white';
	}else if(val=='about'){
		about.style.color='red';
		homeTag.style.color='white';
	}
}
